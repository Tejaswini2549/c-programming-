#include <stdio.h>
#include <stdlib.h>

int main()
 {
    int t;
    scanf("%d",&t);
    while(t--)
    {
    int n;
    scanf("%d",&n);
    long int  a[n],i;
    for(i=0;i<n;i++)
    {
        scanf("%ld",&a[i]);
    }
    long int sum=0;
    for(i=0;i<n;i++)
    {
        sum=sum+a[i];
    }
    printf("%ld\n",sum);
    }
    return 0;
}
